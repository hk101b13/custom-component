#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/weizhaoma/DesignSystem/custom-component/custom-component/node_modules/.pnpm/vite@4.4.5_sass@1.69.3/node_modules/vite/bin/node_modules:/home/weizhaoma/DesignSystem/custom-component/custom-component/node_modules/.pnpm/vite@4.4.5_sass@1.69.3/node_modules/vite/node_modules:/home/weizhaoma/DesignSystem/custom-component/custom-component/node_modules/.pnpm/vite@4.4.5_sass@1.69.3/node_modules:/home/weizhaoma/DesignSystem/custom-component/custom-component/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/weizhaoma/DesignSystem/custom-component/custom-component/node_modules/.pnpm/vite@4.4.5_sass@1.69.3/node_modules/vite/bin/node_modules:/home/weizhaoma/DesignSystem/custom-component/custom-component/node_modules/.pnpm/vite@4.4.5_sass@1.69.3/node_modules/vite/node_modules:/home/weizhaoma/DesignSystem/custom-component/custom-component/node_modules/.pnpm/vite@4.4.5_sass@1.69.3/node_modules:/home/weizhaoma/DesignSystem/custom-component/custom-component/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
